{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@css/*": ["src/assets/css/*"],
      "@vendor/*": ["src/assets/vendor/*"],
      "@js/*": ["src/assets/js/*"],
      "@images/*": ["src/assets/img/*"],
      "@utils/*": ["src/utils/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
